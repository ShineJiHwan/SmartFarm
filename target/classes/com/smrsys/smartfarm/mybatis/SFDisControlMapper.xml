<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SFDisControl">
	
	<select id="selectAll" resultType="SFDisControl">
		select * from SF_Dis_Control
	</select>
	
	<update id="updateValue" parameterType="SFDisControl">
		update SF_Dis_Control set CON_Value=#{CON_Value} where CON_Sequence = #{CON_Sequence} 
	</update>
</mapper>